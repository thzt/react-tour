  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Build",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "debug-build"
      ],
      "port": 5858,
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug React",
      "url": "http://127.0.0.1:3000",
    }
  ]
}